    <bc_top>REFLECT</bc_top>
    <bc_bottom>REFLECT</bc_bottom>
  </spatial>

  <source>
    <source_element>0</source_element>
    <T_source>1.0</T_source>
  </source>

  <material>
    <density>3.0</density>
    <CV>0.1</CV>
    <opacA>0.0</opacA>
    <opacB>100.0</opacB>
    <opacC>3.0</opacC>
    <opacS>0.0</opacS>
