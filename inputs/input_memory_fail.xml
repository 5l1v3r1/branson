    <t_mult>1.0</t_mult>
    <dt_max>1.0</dt_max>
    <Tm_initial>1.0</Tm_initial>
    <Tr_initial>0.0</Tr_initial>
    <photons>10240001</photons>
    <seed>14706</seed>
    <map_size>50000</map_size>
    <tilt>FALSE</tilt>
    <stratified_sampling>FALSE</stratified_sampling>
